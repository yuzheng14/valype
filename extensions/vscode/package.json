{
  "name": "vscode-valype",
  "displayName": "Valype",
  "description": "A VS Code extension for valype.",
  "publisher": "yuzheng14",
  "version": "0.0.11",
  "engines": {
    "vscode": "^1.55.0"
  },
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "dist/extension.js",
  "contributes": {
    "typescriptServerPlugins": [
      {
        "name": "@valype/typescript-plugin",
        "enableForWorkspaceTypeScriptVersions": true,
        "configNamespace": "typescript"
      }
    ]
  },
  "type": "module",
  "devDependencies": {
    "@valype/typescript-plugin": "workspace:*",
    "@types/vscode": "^1.55.0",
    "reactive-vscode": "^0.3.0",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "build": "tsdown"
  },
  "dependencies": {
    "oxc-parser": "^0.77.1"
  }
}
